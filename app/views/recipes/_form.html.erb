<%= form_for @recipe do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <br><br>

  <%= f.fields_for :ingredients do |ing| %>
    <%= ing.label :ingredient %>
    <%= ing.text_field :name %>

    <%= ing.label :quantity %>
    <%= ing.text_field :quantity %>
    <br><br>
  <% end %>

  <%= f.submit %>
<% end %>